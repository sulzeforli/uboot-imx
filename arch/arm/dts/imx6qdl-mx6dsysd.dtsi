			MX6QDL_PAD_GPIO_3__I2C3_SCL                0x4001b8b1
			MX6QDL_PAD_GPIO_6__I2C3_SDA                0x4001b8b1
			>;
	    			};

		pinctrl_ipu1: ipu1grp {
			fsl,pins = <
