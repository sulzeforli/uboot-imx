                	MX6QDL_PAD_SD4_DAT6__SD4_DATA6             0x00017059
                	MX6QDL_PAD_SD4_DAT7__SD4_DATA7             0x00017059
			>;
					};
		
		pinctrl_wdog: wdoggrp {
			fsl,pins = <
			MX6QDL_PAD_GPIO_1__WDOG2_B 0x80000000
			>;
				};
		
		pinctrl_ir1: irgrp {
			fsl,pins= <
 			MX6QDL_PAD_GPIO_8__GPIO1_IO08  0x80000000
			>;
				};
	};

};

&ldb {
	status = "okay";
	split-mode;

	lvds-channel@0 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <24>;
		primary;
		status = "okay";

		display-timings {
			native-mode = <&timing0>;
			timing0: hsd100pxn1 {
				clock-frequency = <128500000>;
				hactive = <1920>;
				vactive = <1080>;
				hback-porch = <220>;
				hfront-porch = <40>;
				vback-porch = <21>;
				vfront-porch = <7>;
				hsync-len = <60>;
				vsync-len = <10>;
			};
		};
	};

	lvds-channel@1 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <18>;
		status = "disabled";

		display-timings {
			native-mode = <&timing1>;
			timing1: hsd100pxn1 {
				clock-frequency = <128500000>;
				hactive = <1920>;
				vactive = <1080>;
				hback-porch = <220>;
				hfront-porch = <40>;
				vback-porch = <21>;
				vfront-porch = <7>;
				hsync-len = <60>;
				vsync-len = <10>;
			};
		};
	};
};

