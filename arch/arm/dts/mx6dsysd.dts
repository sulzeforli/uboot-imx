/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include "imx6dl.dtsi"
#include "imx6qdl-mx6dsysd.dtsi"

/ {
	model = "Talenuse i.MX6DL MX6DSYSD board";
	compatible = "talenuse,mx6dsysd", "fsl,imx6dl";

};

&ldb {
	lvds-channel@0 {
		crtc = "ipu1-di0";
	};

	lvds-channel@1 {
		crtc = "ipu1-di1";
	};
};

&mxcfb1 {
	status = "okay";
};

&mxcfb2 {
	status = "okay";
};

